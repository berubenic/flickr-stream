<%= form_with url: root_path, method: :get do |form| %>
  <%= form.label :profile_id, "Profile ID:" %>
  <%= form.text_field :profile_id %>
  <%= form.submit "Find profile" %>
<% end %>

<% if flash[:errors] %>
  <%= flash[:errors] %>
<% end %>

<% if flash[:alert] %>
  <%= flash[:alert] %>
<% end %>
<br>
<br>
<% if @list.present? %>
    <% @list.each do |photo| %>
        <%= image_tag "https://live.staticflickr.com/#{photo["server"]}/#{photo["id"]}_#{photo["secret"]}_m.jpg" %> <br>
    <% end %>
<% end %>